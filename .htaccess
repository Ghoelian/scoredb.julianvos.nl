RewriteEngine on

RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://scoredb.julianvos.nl/$1 [R,L]

Options -Indexes

RewriteCond %{HTTP_REFERER} !^$
RewriteCond %{HTTP_REFERER} !^http://scoredb.julianvos.nl/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://scoredb.julianvos.nl$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.scoredb.julianvos.nl/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.scoredb.julianvos.nl$      [NC]
RewriteCond %{HTTP_REFERER} !^https://scoredb.julianvos.nl/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://scoredb.julianvos.nl$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.scoredb.julianvos.nl/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.scoredb.julianvos.nl$      [NC]
RewriteRule .*\.()$ - [F,NC]
